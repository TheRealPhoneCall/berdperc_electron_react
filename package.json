{
  "name": "berdperc_app",
  "version": "1.0.0",
  "description": "The desktop app support for configuring/playing with berdperc instruments",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "mocha -R spec",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"berdperc\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": "https://bitbucket.org/DrealPhoneCall/electron_berdperc",
  "keywords": [
    "Electron",
    "berdperc",
    "python arduino node react drums"
  ],
  "author": "berdperc_team",
  "license": "CC0-1.0",
  "devDependencies": {
    "axios": "^0.16.2",
    "babel-loader": "^6.2.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "electron": "~1.6.2",
    "electron-packager": "^9.1.0",
    "eslint": "^4.8.0",
    "eslint-plugin-react": "^7.4.0",
    "history": "^1.17.0",
    "jsonfile": "^4.0.0",
    "materialize-css": "~0.100.2",
    "mocha": "^2.2.5",
    "python-shell": "^0.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-materialize": "^1.0.15",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-redux": "^4.4.5",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0",
    "should": "^6.0.0"
  },
  "dependencies": {   
    
  }
}
